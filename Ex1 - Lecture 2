public class UnitMatrix {
    public static void main(String[] args) {
        int n = 5; 

        for (int row = 0; row < n; row++) {
            for (int col = 0; col < n; j++) {
                if (row == col) {
                    System.out.print("1");
                } else {
                    System.out.print("0");
                }
            }
            System.out.println();
        }
    }
}
